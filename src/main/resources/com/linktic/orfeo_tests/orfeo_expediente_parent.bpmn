    <bpmn2:scriptTask id="_365AF9AA-F359-43DF-ACB2-767B1EBFDB56" name="Get Global" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:metaData name="elementname">
          <drools:metaValue><![CDATA[Get Global]]></drools:metaValue>
        </drools:metaData>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_4BD964D5-FD50-4645-B8B3-40327110214D</bpmn2:incoming>
      <bpmn2:outgoing>_0967E615-76D5-4F0E-8DEC-C6ACD8763CA9</bpmn2:outgoing>
      <bpmn2:script><![CDATA[Globals globals = kcontext.getKieRuntime().getGlobals();
Collection<String> globalKeys = globals.getGlobalKeys();

if (globalKeys != null && globalKeys.size() > 0) {
			
    System.out.println("- Imprimiendo variables globales");
			
    for (Iterator<String> iterator = globalKeys.iterator(); iterator.hasNext();) {
	    String key = (String) iterator.next();
		System.out.println(key + ":" + globals.get(key));
        kcontext.setVariable(key, globals.get(key));
        //kcontext.getKnowledgeRuntime().setGlobal(key, globals.get(key));
	}
}
else {
			
    System.out.println("- No se encontraron variables globales");
}]]></bpmn2:script>
    </bpmn2:scriptTask>
